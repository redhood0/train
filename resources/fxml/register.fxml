<AnchorPane prefHeight="339.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.RegisterController" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="250.0" layoutY="29.0" text="免费注册">
            <font>
                <Font size="25.0"/>
            </font>
        </Label>
        <Label layoutX="162.0" layoutY="92.0" text="新账号">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>

        <TextField fx:id="username" layoutX="220.0" layoutY="91.0"/>

        <Label layoutX="162.0" layoutY="145.0" text="新密码">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="147.0" layoutY="198.0" text="确认密码">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Button layoutX="255.0" layoutY="268.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="90.0"
